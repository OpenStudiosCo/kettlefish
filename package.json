{
  "name": "kettlefish",
  "version": "0.0.2",
  "description": "A Node.JS based boilerplate for no fuss static HTML websites",
  "main": "kettlefish.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/paulbrzeski/kettlefish"
  },
  "keywords": [
    "boilerplate"
  ],
  "scripts": {
    "build": "pug content -P -o . -b . && stylus styles -o dist",
    "css": "stylus styles -o dist -w",
    "pug": "pug content -P -o . -b . -w",
    "rollup": "rollup -c -w",
    "dev-osx": "sudo http-server -p 80 -c-1 .",
    "dev": "http-server -p 80 -c-1 ."
  },
  "author": "Paul Brzeski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paulbrzeski/kettlefish/issues"
  },
  "dependencies": {
    "jeet": "^7.1.0",
    "pug": "^2.0.0-beta6",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "imaginary": "^0.1.12",
    "jstransformer-markdown": "^1.1.1",
    "pug-cli": "^1.0.0-alpha6",
    "rollup": "^0.36.4",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^2.5.0"
  }
}
